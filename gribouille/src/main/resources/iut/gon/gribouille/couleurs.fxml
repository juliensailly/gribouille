<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>

<VBox prefHeight="194.0" prefWidth="116.0" spacing="2.0"
	style="-fx-background-color: #ddd;" xmlns="http://javafx.com/javafx/19"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controleurs.CouleursController"
	fx:id="colorVBox">
	<children>
		<ColorPicker fx:id="sideColorPicker" prefHeight="36.0"
			prefWidth="108.0" />
		<TilePane hgap="2.0" minHeight="-Infinity"
			minWidth="-Infinity" prefColumns="2" prefHeight="311.0" prefRows="4"
			prefTileHeight="50.0" prefTileWidth="50.0" prefWidth="110.0"
			vgap="2.0">
			<padding>
				<Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
			</padding>
			<children>
				<Rectangle fx:id="red" arcHeight="5.0" arcWidth="5.0"
					fill="RED" height="50.0" stroke="BLACK" strokeType="INSIDE"
					width="50.0">
					<TilePane.margin>
						<Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
					</TilePane.margin>
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Rectangle>
				<Rectangle fx:id="green" arcHeight="5.0" arcWidth="5.0"
					fill="LIME" height="50.0" stroke="BLACK" strokeType="INSIDE"
					width="50.0">
					<TilePane.margin>
						<Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
					</TilePane.margin>
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Rectangle>
				<Rectangle fx:id="blue" arcHeight="5.0" arcWidth="5.0"
					fill="BLUE" height="50.0" stroke="BLACK" strokeType="INSIDE"
					width="50.0">
					<TilePane.margin>
						<Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
					</TilePane.margin>
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Rectangle>
				<Rectangle fx:id="cyan" arcHeight="5.0" arcWidth="5.0"
					fill="AQUA" height="50.0" stroke="BLACK" strokeType="INSIDE"
					width="50.0">
					<TilePane.margin>
						<Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
					</TilePane.margin>
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Rectangle>
				<Rectangle fx:id="magenta" arcHeight="5.0" arcWidth="5.0"
					fill="MAGENTA" height="50.0" stroke="BLACK" strokeType="INSIDE"
					width="50.0">
					<TilePane.margin>
						<Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
					</TilePane.margin>
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Rectangle>
				<Rectangle fx:id="yellow" arcHeight="5.0" arcWidth="5.0"
					fill="YELLOW" height="50.0" stroke="BLACK" strokeType="INSIDE"
					width="50.0">
					<TilePane.margin>
						<Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
					</TilePane.margin>
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Rectangle>
				<Rectangle fx:id="black" arcHeight="10.0" arcWidth="10.0"
					height="47.0" stroke="BLACK" strokeType="INSIDE" width="47.0">
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Rectangle>
				<Rectangle fx:id="white" arcHeight="5.0" arcWidth="5.0"
					fill="WHITE" height="50.0" stroke="BLACK" strokeType="INSIDE"
					width="50.0">
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Rectangle>
			</children>
		</TilePane>
	</children>
	<opaqueInsets>
		<Insets />
	</opaqueInsets>
	<padding>
		<Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
	</padding>
</VBox>
